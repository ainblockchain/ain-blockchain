{
  "checkin": {
    "$user_addr": {
      "$checkin_id": {
        "request": {
          ".function": {
            "_openCheckin": {
              "function_type": "NATIVE",
              "function_id": "_openCheckin"
            }
          }
        },
        "parent_finalize": {
          "$tx_hash": {
            "result": {
              ".function": {
                "_closeCheckin": {
                  "function_type": "NATIVE",
                  "function_id": "_closeCheckin"
                }
              }
            }
          }
        }
      }
    }
  },
  "deposit": {
    "$service": {
      "$user_addr": {
        "$deposit_id": {
          "value": {
            ".function": {
              "_deposit": {
                "function_type": "NATIVE",
                "function_id": "_deposit"
              }
            }
          }
        }
      }
    }
  },
  "payments": {
    "$service": {
      "$user_addr": {
        "pays": {
          "$record_id": {
            ".function": {
              "_pay": {
                "function_type": "NATIVE",
                "function_id": "_pay"
              }
            }
          }
        },
        "claims": {
          "$record_id": {
            ".function": {
              "_claim": {
                "function_type": "NATIVE",
                "function_id": "_claim"
              }
            }
          }
        }
      }
    }
  },
  "transfer": {
    "$from": {
      "$to": {
        "$key": {
          "value": {
            ".function": {
              "_transfer": {
                "function_type": "NATIVE",
                "function_id": "_transfer"
              }
            }
          }
        }
      }
    }
  },
  "withdraw":{
    "$service": {
      "$user_addr": {
        "$withdraw_id": {
          "value": {
            ".function": {
              "_withdraw": {
                "function_type": "NATIVE",
                "function_id": "_withdraw"
              }
            }
          }
        }
      }
    }
  }
}
